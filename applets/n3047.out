              char n_cs_precedes;                    // CHAR_MAX
              char p_sep_by_space;                   // CHAR_MAX
              char n_sep_by_space;                   // CHAR_MAX
              char p_sign_posn;                      // CHAR_MAX
              char n_sign_posn;                      // CHAR_MAX
